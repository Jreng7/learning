
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {     // Aqui o "User" é uma representação
  id String @id @default(uuid())
  name String
  email String @unique
  
  @@map("user")  // aqui é o nome da tabela que eu quero usar dentro do banco de dados.
}